<?xml version="1.0" ?>
<sdf version='1.9'>
  <world name='car_world'>
    <physics name='1ms' type='ignored'>
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>1000</real_time_update_rate>
    </physics>
    
    <plugin name='ignition::gazebo::systems::Physics' filename='libignition-gazebo-physics-system.so'/>
    <plugin name='ignition::gazebo::systems::UserCommands' filename='libignition-gazebo-user-commands-system.so'/>
    <plugin name='ignition::gazebo::systems::SceneBroadcaster' filename='libignition-gazebo-scene-broadcaster-system.so'/>
    
    <plugin name="ignition::gazebo::systems::Imu" filename="libignition-gazebo-imu-system.so"/>
    <plugin name='ignition::gazebo::systems::Sensors' filename='libignition-gazebo-sensors-system.so'>
      <render_engine>ogre2</render_engine>
    </plugin>

    <gravity>0 0 -9.8</gravity>
    <magnetic_field>6e-06 2.3e-05 -4.2e-05</magnetic_field>
    <atmosphere type='adiabatic'/>
    <scene>
      <ambient>0.4 0.4 0.4 1</ambient>
      <background>0.7 0.7 0.7 1</background>
      <shadows>true</shadows>
    </scene>

    <model name='ground_plane'>
      <static>true</static>
      <link name='link'>
        <collision name='collision'>
          <geometry><plane><normal>0 0 1</normal><size>1 1</size></plane></geometry>
          <surface><friction><ode/></friction></surface>
        </collision>
        <visual name='visual'>
          <geometry><plane><normal>0 0 1</normal><size>100 100</size></plane></geometry>
          <material><ambient>0.8 0.8 0.8 1</ambient><diffuse>0.8 0.8 0.8 1</diffuse></material>
        </visual>
      </link>
    </model>

    <model name='cylinder_1'><static>true</static><pose>1.5 0 0.25 0 0 0</pose><link name='link'><collision name='c'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry></collision><visual name='v'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry><material><ambient>1 0 0 1</ambient><diffuse>1 0 0 1</diffuse></material></visual></link></model>
    
    <model name='cylinder_2'><static>true</static><pose>1.0 1.0 0.25 0 0 0</pose><link name='link'><collision name='c'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry></collision><visual name='v'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry><material><ambient>0 1 0 1</ambient><diffuse>0 1 0 1</diffuse></material></visual></link></model>

    <model name='cylinder_3'><static>true</static><pose>0 1.5 0.25 0 0 0</pose><link name='link'><collision name='c'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry></collision><visual name='v'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry><material><ambient>0 0 1 1</ambient><diffuse>0 0 1 1</diffuse></material></visual></link></model>

    <model name='cylinder_4'><static>true</static><pose>-1.0 1.0 0.25 0 0 0</pose><link name='link'><collision name='c'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry></collision><visual name='v'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry><material><ambient>1 1 0 1</ambient><diffuse>1 1 0 1</diffuse></material></visual></link></model>

    <model name='cylinder_5'><static>true</static><pose>-1.5 0 0.25 0 0 0</pose><link name='link'><collision name='c'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry></collision><visual name='v'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry><material><ambient>0 1 1 1</ambient><diffuse>0 1 1 1</diffuse></material></visual></link></model>

    <model name='cylinder_6'><static>true</static><pose>-1.0 -1.0 0.25 0 0 0</pose><link name='link'><collision name='c'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry></collision><visual name='v'><geometry><cylinder><radius>0.15</radius><length>0.5</length></cylinder></geometry><material><ambient>1 0 1 1</ambient><diffuse>1 0 1 1</diffuse></material></visual></link></model>


    <model name='turtlebot3_burger'>
      
      <link name='caster_wheel'>
        <pose>-0.095 0 -0.005 0 0 0</pose>
        <inertial>
          <mass>0.005</mass>
          <inertia><ixx>0.0001</ixx><ixy>0</ixy><ixz>0</ixz><iyy>0.0001</iyy><iyz>0</iyz><izz>0.0001</izz></inertia>
        </inertial>
        <collision name='caster_wheel_collision'>
          <geometry><sphere><radius>0.005</radius></sphere></geometry>
          <surface><friction><ode><mu>0.0</mu><mu2>0.0</mu2><slip1>1.0</slip1><slip2>1.0</slip2></ode></friction></surface>
        </collision>
        <visual name='caster_wheel_visual'>
          <geometry><sphere><radius>0.005</radius></sphere></geometry>
          <material><ambient>0.1 0.1 0.1 1</ambient><diffuse>0.1 0.1 0.1 1</diffuse></material>
        </visual>
      </link>

      <link name='base'>
        <inertial>
          <pose>-0.032 0 0.07 0 0 0</pose>
          <mass>1</mass>
          <inertia><ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz><iyy>0.001</iyy><iyz>0</iyz><izz>0.001</izz></inertia>
        </inertial>
        <collision name='base_collision'>
          <pose>-0.032 0 0.070 0 0 0</pose>
          <geometry><box><size>0.14 0.14 0.14</size></box></geometry>
        </collision>
        <visual name='base_visual'>
          <pose>-0.032 0 0 0 0 0</pose>
          <geometry>
            <mesh>
              <uri>file:///home/amanu/.ignition/fuel/fuel.gazebosim.org/cholorsplash/models/turtlebot 3 burger/1/meshes/burger_base.stl</uri>
              <scale>0.001 0.001 0.001</scale>
            </mesh>
          </geometry>
          <material><ambient>0 0 0 1</ambient><diffuse>0 0 0 1</diffuse></material>
        </visual>
        
        <sensor name="imu_sensor" type="imu">
            <always_on>1</always_on>
            <update_rate>50</update_rate>
            <visualize>true</visualize>
            <topic>imu</topic>
        </sensor>
      </link>

      <link name="camera">
        <pose>0.05 0 0.12 0 0 0</pose> <inertial>
          <mass>0.08</mass>
          <inertia><ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz><iyy>0.001</iyy><iyz>0</iyz><izz>0.001</izz></inertia>
        </inertial>
        <visual name='camera_visual'>
          <geometry><box><size>0.012 0.03 0.012</size></box></geometry>
          <material><ambient>0 0 0 1</ambient><diffuse>0 0 0 1</diffuse></material>
        </visual>
        <sensor type="camera" name="camera_sensor">
          <topic>camera/image</topic> <camera>
            <horizontal_fov>1.047</horizontal_fov>
            <image><width>640</width><height>480</height></image>
            <clip><near>0.1</near><far>100</far></clip>
          </camera>
          <always_on>1</always_on>
          <update_rate>30</update_rate>
          <visualize>true</visualize>
        </sensor>
      </link>

      <link name='lidar'>
        <inertial>
          <pose>-0.02 0 0.161 0 0 0</pose>
          <mass>0.125</mass>
          <inertia><ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz><iyy>0.001</iyy><iyz>0</iyz><izz>0.001</izz></inertia>
        </inertial>
        <visual name='lidar_sensor_visual'>
          <pose>-0.032 0 0.171 0 0 0</pose>
          <geometry>
            <mesh>
              <uri>file:///home/amanu/.ignition/fuel/fuel.gazebosim.org/cholorsplash/models/turtlebot 3 burger/1/meshes/lds.stl</uri>
              <scale>0.001 0.001 0.001</scale>
            </mesh>
          </geometry>
          <material><ambient>0.2 0.2 0 1</ambient><diffuse>0.2 0.2 0 1</diffuse></material>
        </visual>
        <sensor name='hls_lfcd_lds' type='gpu_lidar'>
          <pose>-0.032 0 0.171 0 0 0</pose>
          <topic>scan</topic>
          <update_rate>10</update_rate>
          <visualize>true</visualize> <lidar>
            <scan>
              <horizontal><samples>360</samples><resolution>1</resolution><min_angle>0</min_angle><max_angle>6.28</max_angle></horizontal>
              <vertical><samples>1</samples><min_angle>0</min_angle><max_angle>0</max_angle></vertical>
            </scan>
            <range><min>0.12</min><max>3.5</max></range>
          </lidar>
        </sensor>
      </link>

      <link name='left_wheel'>
        <inertial>
          <pose>0 0.08 0.023 -1.57 0 0</pose>
          <mass>0.1</mass>
          <inertia><ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz><iyy>0.001</iyy><iyz>0</iyz><izz>0.001</izz></inertia>
        </inertial>
        <collision name='left_wheel_collision'>
          <pose>0 0.08 0.023 -1.57 0 0</pose>
          <geometry><cylinder><radius>0.033</radius><length>0.018</length></cylinder></geometry>
          <surface><friction><ode><mu>100000</mu><mu2>100000</mu2></ode></friction></surface>
        </collision>
        <visual name='left_wheel_visual'>
          <pose>0 0.08 0.023 0 0 0</pose>
          <geometry>
             <mesh>
              <uri>file:///home/amanu/.ignition/fuel/fuel.gazebosim.org/cholorsplash/models/turtlebot 3 burger/1/meshes/left_tire.stl</uri>
              <scale>0.001 0.001 0.001</scale>
            </mesh>
          </geometry>
          <material><ambient>0.1 0.1 0.1 1</ambient><diffuse>0.1 0.1 0.1 1</diffuse></material>
        </visual>
      </link>

      <link name='right_wheel'>
        <inertial>
          <pose>0 -0.08 0.023 -1.57 0 0</pose>
          <mass>0.1</mass>
          <inertia><ixx>0.001</ixx><ixy>0</ixy><ixz>0</ixz><iyy>0.001</iyy><iyz>0</iyz><izz>0.001</izz></inertia>
        </inertial>
        <collision name='right_wheel_collision'>
          <pose>0.0 -0.08 0.023 -1.57 0 0</pose>
          <geometry><cylinder><radius>0.033</radius><length>0.018</length></cylinder></geometry>
          <surface><friction><ode><mu>100000</mu><mu2>100000</mu2></ode></friction></surface>
        </collision>
        <visual name='right_wheel_visual'>
           <pose>0.0 -0.08 0.023 0 0 0</pose>
          <geometry>
             <mesh>
              <uri>file:///home/amanu/.ignition/fuel/fuel.gazebosim.org/cholorsplash/models/turtlebot 3 burger/1/meshes/right_tire.stl</uri>
              <scale>0.001 0.001 0.001</scale>
            </mesh>
          </geometry>
          <material><ambient>0.1 0.1 0.1 1</ambient><diffuse>0.1 0.1 0.1 1</diffuse></material>
        </visual>
      </link>
      
      <joint name='caster_wheel_joint' type='fixed'>
        <parent>base</parent><child>caster_wheel</child>
      </joint>
      <joint name='camera_joint' type='fixed'>
        <parent>base</parent><child>camera</child>
      </joint>
      <joint name='left_wheel_joint' type='revolute'>
        <parent>base</parent><child>left_wheel</child>
        <pose>0 0.08 0.023 -1.57 0 0</pose>
        <axis><xyz>0 0 1</xyz></axis>
      </joint>
      <joint name='right_wheel_joint' type='revolute'>
        <parent>base</parent><child>right_wheel</child>
        <pose>0 -0.08 0.023 -1.57 0 0</pose>
        <axis><xyz>0 0 1</xyz></axis>
      </joint>
      <joint name='lidar_joint' type='fixed'>
        <parent>base</parent><child>lidar</child>
      </joint>

      <plugin filename="libignition-gazebo-joint-state-publisher-system.so" 
              name="ignition::gazebo::systems::JointStatePublisher"/>

      <plugin name='ignition::gazebo::systems::DiffDrive' filename='ignition-gazebo-diff-drive-system'>
        <left_joint>left_wheel_joint</left_joint>
        <right_joint>right_wheel_joint</right_joint>
        <wheel_separation>0.16</wheel_separation> <wheel_radius>0.033</wheel_radius>
        <odom_publish_frequency>20</odom_publish_frequency>
        <frame_id>odom</frame_id>
        <child_frame_id>base</child_frame_id>
        <topic>cmd_vel</topic>
      </plugin>
      
      <static>false</static>
      <pose>0 0 0.010 -0 -0 -0</pose>
    </model>

    <light name='sun' type='directional'>
      <pose>0 0 10 0 -0 0</pose>
      <cast_shadows>true</cast_shadows>
      <intensity>1</intensity>
      <direction>-0.5 0.1 -0.9</direction>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
    </light>
  </world>
</sdf>